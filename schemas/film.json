{
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string",
      "nullable": true
    },
    "release_year": {
      "type": "integer",
      "minimum": 1901,
      "maximum": 2155,
      "nullable": true
    },
    "language_id": {
      "type": "integer",
      "minimum": 1,
      "maximum": 255
    },
    "original_language_id": {
      "type": "integer",
      "minimum": 1,
      "maximum": 255,
      "nullable": true
    },
    "rental_duration": {
      "type": "integer",
      "minimum": 1,
      "maximum": 255
    },
    "rental_rate": {
      "type": "number",
      "exclusiveMinimum": 0,
      "exclusiveMaximum": 100
    },
    "length": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535,
      "nullable": true
    },
    "replacement_cost": {
      "type": "number",
      "exclusiveMinimum": 0,
      "exclusiveMaximum": 1000
    },
    "rating": {
      "enum": ["G", "PG", "PG-13", "R", "NC-17"]
    },
    "special_features": {
      "type": "string",
      "set": ["Trailers", "Commentaries", "Deleted Scenes", "Behind the Scenes"],
      "nullable": true
    }
  },
  "required": ["title", "language_id"],
  "additionalProperties": false
}